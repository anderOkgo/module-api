{
  "name": "modulapi",
  "version": "3.0.0",
  "description": "This API provides different services like modules.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:cov:watch": "jest --coverage --watch",
    "test:cov:html": "jest --coverage --coverageReporters=html",
    "test:cov:threshold": "jest --coverage --coverageThreshold='{\"global\":{\"statements\":80,\"branches\":80,\"functions\":80,\"lines\":80}}'",
    "test:cov:auth": "jest --coverage --testPathPattern=modules/auth",
    "test:cov:finan": "jest --coverage --testPathPattern=modules/finan",
    "test:cov:series": "jest --coverage --testPathPattern=modules/series",
    "build": "tsc",
    "servers": "start /b tsc -w & nodemon dist/index.js",
    "bup": "npm run build && call com/db-sync-FTP-both.bat"
  },
  "keywords": [],
  "author": "Anderokgo",
  "license": "ISC",
