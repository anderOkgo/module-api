    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/mysql": "^2.15.21",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.15",
    "@types/sharp": "^0.31.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "typescript": "^4.9.5"
  }
